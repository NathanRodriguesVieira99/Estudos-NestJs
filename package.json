{
  "name": "estudos_nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:up": "docker compose up -d",
    "docker:stop": "docker compose stop",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test run --bail 1",
    "test:watch": "dotenv -e .env.test -- vitest --bail 1",
    "test:coverage": "dotenv -e .env.test -- vitest run --coverage --no-file-parallelism --passWithNoTests",
    "test:debug": "dotenv -e .env.test -- vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "dotenv -e .env.e2e -- vitest run --config ./vitest.config.e2e.ts",
    "test:coverage:e2e": "dotenv -e .env.e2e -- vitest run --config vitest.config.e2e.ts --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.11.1",
    "bcryptjs": "^3.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^9.0.0",
    "eslint": "^9.31.0",
    "globals": "^16.0.0",
    "prisma": "^6.11.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-swc": "^1.5.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
